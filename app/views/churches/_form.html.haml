= form_for church do |f|
  - if church.errors.any?
    %h2 Errors:
    %ul
      - church.errors.full_messages.each do |msg|
        %li= msg

  = f.label      :name
  = f.text_field :name
  = f.label      :denomination_list
  = f.text_field :denomination_list
  = f.label      :tag_list
  = f.text_field :tag_list
  = f.submit
